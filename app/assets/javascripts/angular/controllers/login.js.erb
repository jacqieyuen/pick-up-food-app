app.controller('LogInCtrl', ['$scope', '$auth', 'UserService', '$location', function($scope, $auth, UserService, $location){

  $scope.login = function () {
    $auth.submitLogin($scope.loginForm).
    then(function(resp) {
      console.log(resp);
      $location.path('/');
    }).catch(function(resp) {
      console.log(resp);
    });
  };

}])